Author = {Y. Ren, M. Yamada, H. Ji, S.P. Gerhardt, and R. Kulsrud},
Title = {{Identification of the electron-diffusion region during magnetic
   reconnection in a laboratory plasma}},
Journal = PRL,
Year = {{2008}},
Volume = {{101}},
Number = {{8}},
Month = {{AUG 22}},
Abstract = {{We report the first identification of the electron-diffusion region,
   where demagnetized electrons are accelerated to super-Alfvenic speed,
